{
    "name": "boost/craft-multie",
    "description": "Simple multi-site management tools.",
    "type": "craft-plugin",
    "version": "3.0.0",
    "license": "proprietary",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "multie"
    ],
    "support": {
        "email": "matthewdejager5@gmail.com",
        "issues": "https://github.com/MattDeJager/Multie/issues?state=open",
        "source": "https://github.com/MattDeJager/Multie",
        "docs": "https://github.com/MattDeJager/Multie",
        "rss": "https://github.com/MattDeJager/Multie/releases.atom"
    },
    "require": {
        "php": ">=7.3",
        "craftcms/cms": "^5.0.0"
    },
    "require-dev": {
        "craftcms/ecs": "dev-main",
        "craftcms/phpstan": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "boost\\multie\\": "src/"
        }
    },
    "extra": {
        "handle": "multie",
        "name": "Multie",
        "developer": "Boost",
        "documentationUrl": "https://github.com/MattDeJager/Multie"
    },
    "scripts": {
        "check-cs": "ecs check --ansi",
        "fix-cs": "ecs check --ansi --fix",
        "phpstan": "phpstan --memory-limit=1G"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
